package hw3;

import java.util.ArrayList;

public class IDLList<E> {
	private Node<E> head;
	private Node<E> tail;
	private int size;
	private ArrayList<Node<E>> indices;
	public IDLList() {
		
	}
	
	private class Node<E> {
		E data;
		Node<E> next;
		Node<E> prev;
		
		Node(E data) {
			this.data = data;
		}
		
		Node(E data, Node<E> prev, Node<E> next) {
			this.data = data;
			this.prev = prev;
			this.next = next;
		}
		
	}

}
