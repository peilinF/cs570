package hw3;

public class hw3 {
	
	public static void main (String[] args) throws Exception {
		IDLList<Integer> a = new IDLList<Integer>();
		a.add(0);
		a.add(1);
		a.add(2);
		a.add(3);
		a.append(4);
		System.out.println(a);
		//a.remove();
		//a.removeLast();
		a.remove();
		System.out.println(a);
		a.removeLast();
		System.out.println(a);
	}
}
