package hw3;

public class hw3 {
	
	public static void main (String[] args) throws Exception {
		IDLList<Integer> a = new IDLList<Integer>();
		a.add(1);
		a.add(2);
		a.add(0,0);
		a.append(3);
		a.append(4);
		a.remove();
		a.removeLast();
		//a.remove(0);
		//a.remove(Integer.valueOf(1));
		System.out.println(a);
	}
}
